{
  "name": "web",
  "description": "A web client",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config config/webpack/config.js",
    "build:watch": "webpack --watch --config config/webpack/config.js",
    "start": "run-p build:watch start:dev",
    "start:dev": "webpack-serve --open --config ./config/webpack/config.js --port 8000",
    "test": "run-p test:*",
    "test:jest": "jest --coverage --config config/jest/config.json",
    "test:tslint": "tslint --format verbose --project tsconfig.json"
  },
  "dependencies": {
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "ts-loader": "4.1.0",
    "tslib": "1.9.0",
    "webpack": "4.1.1"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.9",
    "@types/enzyme-adapter-react-16": "1.0.2",
    "@types/jest": "22.2.0",
    "@types/node": "9.4.7",
    "@types/prop-types": "15.5.3",
    "@types/react": "16.0.40",
    "@types/react-dom": "16.0.4",
    "@types/react-redux": "5.0.21",
    "@types/redux": "3.6.0",
    "@types/webpack": "4.1.1",
    "awesome-typescript-loader": "4.0.1",
    "jest": "22.4.2",
    "jest-enzyme": "6.0.0",
    "npm-run-all": "4.1.2",
    "prettier": "1.11.1",
    "shx": "0.2.2",
    "source-map-loader": "0.2.3",
    "ts-jest": "22.4.1",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "tslint-config-prettier": "1.10.0",
    "tslint-react": "3.5.1",
    "typescript": "2.7.2",
    "webpack": "4.1.1",
    "webpack-cli": "2.0.12",
    "webpack-serve": "0.2.0"
  }
}
